body{
	padding: 10px 50px 20px 50px;
}

.navbar-default .navbar-toggle .icon-bar {
    background-color: #2c3e50;
}
.navbar-custom {
	/*background-color: #ffffff;*/
	background-color: #2c3e50;
    color:#ffffff;
  	border-radius:0;
    min-height:auto;
    font-size: 105%;
    /*border-bottom: 2px solid #1ABC9C;*/
    font-family: Tahoma, Verdana, Segoe, sans-serif;
}
  
.navbar-custom .navbar-nav > li > a {
  	color:#ffffff;
  	padding-left:25px;
  	padding-right:25px;
    padding-top:20px;
    padding-bottom:20px;
    font-size: 105%;
    font-family: Tahoma, Verdana, Segoe, sans-serif;
}

.navbar-custom .navbar-nav > .active > a, .navbar-nav > .active > a:hover, .navbar-nav > .active > a:focus {
    color: #1ABC9C;
   	border-bottom-color: #ffffff;
}
      
.navbar-custom .navbar-nav > li > a:hover, .nav > li > a:focus {
	color: #2c3e50;
   	border-bottom-color: #ffffff;
}
      
.navbar-custom .navbar-brand {
   color:#A6A497;
   padding:20px;
}

.navbar-custom .navbar-toggle,
.navbar-custom .nav .open>a, .navbar-custom .nav .open>a:hover, .navbar-custom .nav .open>a:focus {
   	color: #2c3e50;
   	border-bottom-color: #ffffff;
}	

.navbar-custom .icon-bar {
   border: 1px solid #1ABC9C;                
}

.navbar-custom .navbar-nav .label-custom{
	background-color: #1ABC9C;
}

/*-- change navbar dropdown color --*/
.navbar-custom .navbar-nav .open .dropdown-menu>li>a,.navbar-custom .navbar-nav .open .dropdown-menu {
    color:#1abc9c;
}

#sidebar {
	padding-top:60px;
    width:230px;
    position:fixed;
    top:0; bottom:0; left:0;
    background:#1abc9c;
    min-height:100%;
	height:auto !important;
	height:100%;
	overflow:scroll;
	font-family: Geneva, Tahoma, Verdana, sans-serif;	
}

#sidebarInitials{
	font-family: Palatino, "Palatino Linotype", "Palatino LT STD", "Book Antiqua", Georgia, serif;
}

#topRow{
	padding-left: 200px;
	padding-top: 40px;
}

hr{
border-top: 1px solid #A6A497;
}

#bottomRow{
	padding-left: 230px;
}

.panel-custom{
	padding-top: 30px;
	padding-left: 150px;
    color: #2c3e50;
    background-color: #ffffff;
    border-color: #ffffff;
}

.panel-custom .panel-footer{
	color: #2c3e50;
    background-color: #1ABC9C;
}

/*-- project detail --*/
	.projectTitle {
		position: relative;
		bottom: 0px;
		padding-top: 25px;
		padding-bottom: 10px;
		padding-left: 25px;
	}
	.projectTitle h1{
		color: #2c3e50;
		font-size: 22px;
		font-weight: 900;
		margin: 0 !important;
		padding: 0 !important;
	}
	.projectTitle h3 {
		color: #1ABC9C;
		font-size: 15px;
		font-weight: 400;
		margin-top: 5px;
	}
	
	.docTitle {
		position: relative;
		bottom: 0px;
		padding-top: 0;
		padding-bottom: 25px;
		padding-left: 25px;
	}
	
	
	
	
	#timeline-embed{
		height: 430px !important;
	}
	
	.modal .modal-header {
	  border-bottom: none;
	  position: relative;
	}
	.modal .modal-header .btn {
	  position: absolute;
	  top: 0;
	  right: 0;
	  margin-top: 0;
	  border-top-left-radius: 0;
	  border-bottom-right-radius: 0;
	}
	
	.modal .modal-footer{
		border-top: none;
	}
	.modal .modal-footer .btn-group > .btn:first-child {
	  border-bottom-left-radius: 0;
	}
	.modal .modal-footer .btn-group > .btn:last-child {
	  border-top-right-radius: 0;
	}
	
	#createProject .modal-body {
		position: relative;
    	height: 580px;
	}
	
	#editProject .modal-body {
		position: relative;
    	height: 580px;
	}
	
	#addCard .modal-body {
		position: relative;
	    height: 450px;
	}
	
	#editCard .modal-body{
		position: relative;
	    height: 450px;
	}
		
	.nav-tabs-custom{
	padding-top: 25px;
	}
	